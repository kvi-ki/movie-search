{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"module": "esnext",
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "preserve",
		"strict": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"baseUrl": "./",
		"paths": {},
		"esModuleInterop": true,
		"allowJs": true,
		"incremental": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"jest.config.cjs"
	],
	"exclude": ["node_modules"]
}
